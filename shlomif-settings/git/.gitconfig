[user]
    name = Shlomi Fish
    email = shlomif@shlomifish.org

[alias]
    ci = commit
    co = checkout
    dh = diff HEAD
    di = diff
    st = status
    cp = cherry-pick
    au = add -u

[core]
    excludesfile = ~/conf/git/excludes
[push]
    default = simple
[url "git://anongit.kde.org/"]
	insteadOf = kde:
[url "git@git.kde.org:"]
	pushInsteadOf = kde:
[color]
	ui = false
